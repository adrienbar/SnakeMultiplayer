<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.snakeindustry.snakemultiplayer" >

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar" >

        <!-- android:theme="@style/AppTheme" -->
        <activity
            android:name=".generalApp.mainActivity.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".generalApp.player.ProfileActivity"
            android:label="@string/title_activity_profile" >
        </activity>
        <activity
            android:name=".generalApp.NetworkModeAndGameSettings"
            android:label="@string/title_activity_network_mode_and_game_settings" >
        </activity>
        <activity
            android:name=".generalApp.player.stats.StatsOneGameActivity"
            android:label="@string/title_activity_game_stats" >
        </activity>
        <activity
            android:name=".generalApp.player.EditName"
            android:label="@string/title_activity_edit_name" >
        </activity>
        <activity
            android:name=".Snake.viewAndControl.GamePlayActivity"
            android:label="@string/title_activity_game_play" >
        </activity>
        <activity
            android:name=".generalApp.pseudoNetwork.RoomActivity"
            android:label="@string/title_activity_room" >
        </activity>
        <activity
            android:name=".generalApp.pseudoNetwork.RoomActivityClient"
            android:label="@string/title_activity_room_Client" >
        </activity>
        <activity
            android:name=".Snake.Activity_to_delete"
            android:label="@string/title_activity_snake_settings" >
        </activity>
        <activity
            android:name=".generalApp.player.settings.SettingsOneGameActivity"
            android:label="@string/title_activity_settings_one_game" >
        </activity>
    </application>

</manifest>
